{
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
          "experimentalObjectRestSpread": true
        }
    },

    "env": {
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ]
}